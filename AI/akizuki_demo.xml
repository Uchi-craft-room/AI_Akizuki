			<pennant-number>DD-115</pennant-number>
			<TACAN-channel-ID>033Y</TACAN-channel-ID>
			<model>Models/Geometry/akizuki/akizuki.xml</model>

			<latitude>35.4478</latitude>
			<longitude>139.7250</longitude>
			<heading>170</heading>
<!--
			<latitude>34.9</latitude>
