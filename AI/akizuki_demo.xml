			<rudder-constant>30</rudder-constant>
			<speed-constant>5</speed-constant>
			<max-speed-kts>120</max-speed-kts>
			<station>
				<range-nm>0.10</range-nm>
				<brg-deg>-150</brg-deg>
				<range-limit-nm>0</range-limit-nm>
				<height-ft>60</height-ft>
